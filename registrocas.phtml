<script language="javascript"> 
function validar() {

nombre= document.form8.nombre.value;
apellido= document.form8.apellido.value;
direccion = document.form8.direccion.value;
telefono = document.form8.tel.value;
email = document.form8.email.value;
email2 = document.form8.email2.value;

ciudad = document.form8.ciudad.value;
estado = document.form8.estado.value;
pais = document.form8.pais.value;
password= document.form8.password.value;
password1= document.form8.password1.value;


	if( nombre == null || nombre.length == 0 || /^\s+$/.test(nombre) ) {
		alert('[ERROR] Nombre no puede esta vacio');
	 return false;
	}

	else if(apellido== null || apellido.length == 0 || /^\s+$/.test(apellido) ) {
		alert('[ERROR] Apellido  Seleccione no puede estar vacio');
		 return false;
		}
	
	else if( direccion  == null || direccion.length == 0 || /^\s+$/.test(direccion) ) {
		alert('[ERROR] Direccion  Seleccione no puede estar vacio');
		 return false;
		}

	else if (isNaN(telefono)|| telefono == null || telefono.length == 0 || /^\s+$/.test(telefono) ) {
	      // Si no se cumple la condicion...
	      alert('[ERROR] Telefono Solo numero no puede estar vacio');
	      return false;
	      }

	else if (email == null || email.length == 0 || /^\s+$/.test(email)){
	      // Si no se cumple la condicion...
	      alert('[ERROR] Email no puede estar vacio');
	      return false;
	      }


	
		else if( ciudad  == null || ciudad.length == 0 || /^\s+$/.test(ciudad) ) {
		alert('[ERROR]ciudad  Seleccione No puede estar vacio');
		 return false;
		}

	else if( estado  == null || estado.length == 0 || /^\s+$/.test(estado) ) {
		alert('[ERROR] Estado  Seleccione no puede estar vacio');
		 return false;
		}

	else if( pais == null || pais.length == 0 || /^\s+$/.test(pais) ) {
		alert('[ERROR] Pais no puede estar vacio');
		 return false;
		}

	
	else if(password== null ||password.length == 0 || /^\s+$/.test(password)) {
		alert('[ERROR] Password no puede esta vacio primera casilla');
		 return false;
		}

	if(password != password1){
		   alert('[ERROR] Password o clave no son iguales');
			 return false;
		   }

	elemento = document.getElementById("terminos");
	if( !elemento.checked ) {
		 alert('[ERROR] Aceptar Terminos y condiciones');
	  return false;
	}

	if(email != email2){
		   alert('[ERROR] Email no son iguales');
			 return false;
		   }

	

	
	

  return true;




  
}
</script>

