<script type="text/javascript">
$(document).ready(function(){
	cargar_paises();
	$("#pais").change(function(){dependencia_estado();});
	$("#estado").change(function(){dependencia_ciudad();});
	
	$("#estado").attr("disabled",true);
	$("#ciudad").attr("disabled",true);
	
});

function cargar_paises()
{
	$.get("<?=$this->baseUrl()?>/web/webfunciones/listapaisesweb", function(resultado){
		if(resultado == false)
		{
			alert("Seleccione pais");
		}
		else
		{
			$('#pais').append(resultado);			
		}
	});	
}
function dependencia_estado()
{
	var code = $("#pais").val();
	$.get("<?=$this->baseUrl()?>/web/webfunciones/cargarestadosweb", { code: code },
		function(resultado)
		{
			if(resultado == false)
			{
				alert("Seleccione Estado");
			}
			else
			{
				$("#estado").attr("disabled",false);
				document.getElementById("estado").options.length=1;
				$('#estado').append(resultado);			
			}
		}

	);
}

function dependencia_ciudad()
{
	var code = $("#estado").val();
	$.get("<?=$this->baseUrl()?>/web/webfunciones/cargarciudades", { code: code },
	  function(resultado){
		if(resultado == false)
		{
			alert("Cargar Ciudad");
		}
		else
		{
			$("#ciudad").attr("disabled",false);
			document.getElementById("ciudad").options.length=1;
			$('#ciudad').append(resultado);			
		}
	});	
}





	
</script>



